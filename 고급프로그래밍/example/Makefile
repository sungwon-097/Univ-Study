RUNFILE5 = 5
RUNFILE6 = 6
RUNFILE12 = 12
RUNFILE15 = 15
RUNFILE22 = 22
OBJECTS = g++ -o
FILE = .cpp
OPTION1 = -std=c++11 -stdlib=libc++

all:
	make 5
	make 6
	make 12
	make 15
	make 22
5:
	$(OBJECTS) $(RUNFILE5) $(RUNFILE5)$(FILE)
	  ./$(RUNFILE5)
	
6:
	$(OBJECTS) $(RUNFILE6) $(RUNFILE6)$(FILE)
	./$(RUNFILE6)
	
12:
	$(OBJECTS) $(RUNFILE12) $(RUNFILE12)$(FILE)
	./$(RUNFILE12)

15:
	$(OBJECTS) $(RUNFILE15) $(RUNFILE15)$(FILE)
	./$(RUNFILE15)
22:
	$(OBJECTS) $(RUNFILE22) $(RUNFILE22)$(FILE)
	./$(RUNFILE22)

clean:
	rm $(RUNFILE5) $(RUNFILE6) $(RUNFILE12) $(RUNFILE15) $(RUNFILE22) $(RUNFILE23)